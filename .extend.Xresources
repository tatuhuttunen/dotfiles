! special
*.background:   #1E2127
*.foreground:   #ABB2BF
*.cursorColor:  #5C6370

 

! black
*.color0:       #1E2127
*.color8:       #5C6370

! red
*.color1:       #E06C75
*.color9:       #E06C75

! green
*.color2:       #98C379
*.color10:      #98C379

! yellow
*.color3:       #D19A66
*.color11:      #D19A66

! blue
*.color4:       #61AFEF
*.color12:      #61AFEF

! magenta
